{
  "name": "dyslexia-text-assistant",
  "displayName": "Dyslexia Text Assistant",
  "description": "Text assistant tailored to kids with dyslexia.",
  "version": "1.1.0",
  "publisher": "hakonlindquist",
  "icon": "kid-writing.png",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hlindquist/vscode-spelling"
  },
  "sideEffects": [
    ".css"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dyslexia-text-assistant.assistantView",
        "title": "Text Assistant"
      }
    ],
    "configuration": {
      "title": "Dyslexia Text Assistant Configuration",
      "properties": {
        "dyslexiaTextAssistant.openAiApiKey": {
          "type": "string",
          "default": "",
          "description": "Set your OpenAI API key (openai.com)"
        },
        "dyslexiaTextAssistant.language": {
          "type": "string",
          "default": "english",
          "enum": [
            "english",
            "norwegian"
          ],
          "enumDescriptions": [
            "English",
            "Norwegian"
          ],
          "description": "Select in what language your texts are written"
        }
      }
    }
  },
  "activationEvents": [
    "onCommand:dyslexia-text-assistant.assistantView"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack",
    "serve-localweb": "webpack serve --config webpack.localweb.config.js --mode development",
    "compile-localweb": "webpack --config webpack.localweb.config.js --mode development",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@types/diff": "^5.0.3",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.198",
    "@types/node": "20.2.5",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/vscode": "1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vscode/test-electron": "^2.3.2",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^8.1.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@types/ramda": "^0.29.5",
    "@types/styled-components": "^5.1.27",
    "axios": "^1.4.0",
    "diff": "^5.1.0",
    "lodash": "^4.17.21",
    "ramda": "^0.29.0",
    "styled-components": "^6.0.8",
    "use-debounce": "^9.0.4"
  }
}
